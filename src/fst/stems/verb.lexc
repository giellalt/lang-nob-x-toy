    s:se Verb_Se ; !to see
    led Verb_lede ; !to lead
    kom Verb_Kom ; !to come
    k:kunner Verb_kunner ; !to can, could, know
    invader Verb_invader ; !to invade
    
LEXICON Verb_Reg
    +V+Inf:e # ;
